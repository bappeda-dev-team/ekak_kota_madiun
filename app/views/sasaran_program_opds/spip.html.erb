<div class="row">
  <div class="col-12">
    <nav aria-label="breadcrumb" class="d-none d-md-inline-block mb-3">
      <ol class="breadcrumb breadcrumb-dark breadcrumb-transparent mb-0">
        <li class="breadcrumb-item">
          <%= link_to root_path do %>
            <i class="fas fa-home"></i>
          <% end %>
        </li>
        <li class="breadcrumb-item active" aria-current="page">SPIP</li>
      </ol>
    </nav>
    <% flash.each do |type, msg| %>
      <% tipe = type == 'error' ? 'danger' : type %>
      <div class="alert alert-<%= tipe %>">
        <%= msg %>
      </div>
    <% end %>
    <%= render 'shared/filter_card', filter_url: '/filter_opd', filter_file: 'sasaran_program_opds' %>
    <div class="card border-0 shadow" id="hasil_filter_opd">
      <div class="card-header">
        <div class="row align-items-center">
          <div class="col">
            <h2 class="fs-5 fw-bold mb-0">SPIP</h2>
          </div>
        </div>
      </div>
      <div class="card-body">
        <%# hasil filter %>
        ...
      </div>
    </div>
  </div>
</div>
