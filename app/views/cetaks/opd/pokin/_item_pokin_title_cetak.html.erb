      <h5 class="text-center bg-danger border border-3 border-primary p-2 mb-2"><strong>Non-Aktif</strong></h5>
    <% end %>
  <% end %>
  <h5 class="text-center"><strong><%= pohon.title_up %></strong></h5>
</div>
