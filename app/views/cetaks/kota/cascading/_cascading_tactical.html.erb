<% presenter = PohonKotaPresenter.new(pohon) %>
<li>
  <div class="tf-nc tactical-pohon" style="width: 450px;">
    <%= render partial: 'cetaks/kota/cascading/item_cascading_tematik', locals: { presenter: presenter } %>
  </div>
  <% collections = @operational_tematiks.select { |str| str.pohon_ref_id == pohon.id } %>
  <% if collections.any? %>
    <ul class='childs'>
      <%= render partial: 'cetaks/kota/cascading/cascading_operational',
                 collection: collections,
                 as: :pohon %>
    </ul>
  <% end %>
</li>
